namespace Intro_Ex2
{    
    class Program
    {
        static void Main(string[] args)
        {
            // 코멘트: 한 라인 코멘트는 두개의 슬래시 사용함       
            int a = 1;


            int b = 1;  // 코멘트: 하나의 문장 뒤에 코멘트를 달 수 있음

            /*
               복수 라인에 대한 코멘트
               int c;
               int d; 
            */
        }
    }
}